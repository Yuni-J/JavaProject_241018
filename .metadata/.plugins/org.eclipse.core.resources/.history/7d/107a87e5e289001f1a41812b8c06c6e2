package day10;

import java.io.File;

public class File06 {

	public static void main(String[] args) {
		// File 객체 : 경로 + 확장자
		// D:\yuni_240725\java_workspace\java_project\test.txt
		// 파일 생성
		//File.separator : 파일 경로 구분자(운영체제마다 다름) \\ /
		
		File f = new File("D:\\yuni_240725\\java_workspace\\java_project\\test.txt");
		System.out.println(File.separator);
		

	}

}
