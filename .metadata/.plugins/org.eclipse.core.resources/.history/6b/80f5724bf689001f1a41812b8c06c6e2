package word_1014;

public class Word {
	private String name;
	private String mean;
	
	public Word() {}
	public Word(String name, String mean) {
		this.name = name;
		this.mean = mean;
	}

	@Override
	public String toString() {
		return name+"\r\n"+ ": "+mean ;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getMean() {
		return mean;
	}
	public void setMean(String mean) {
		this.mean = mean;
	}
}
