package day09;

public class Student {
	
	private String name;
	private int score;
	
	@Override
	public String toString() {
		return name + " : " + score;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getScore() {
		return score;
	}
	public void setScore(int score) {
		this.score = score;
	}
}
